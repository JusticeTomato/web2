{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
      "UVICORN_CMD": "uvicorn main:app --host 0.0.0.0 --port $PORT"
    }
  }